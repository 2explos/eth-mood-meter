# Chain ID (8453 for Base mainnet, 84532 for Base Sepolia)
NEXT_PUBLIC_CHAIN_ID=8453

# EthMoodMeter Contract Address on Base
NEXT_PUBLIC_CONTRACT=0xYourContractAddressHere
